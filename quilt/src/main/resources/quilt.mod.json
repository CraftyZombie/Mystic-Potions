{
  "schema_version": 1,
  "mixin": [
    "mysticpotions.mixins.json",
    "mysticpotions-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "mysticpotions",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.creep3rcrafter.mysticpotions.quilt.MysticPotionsQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=${minecraftVersion}"
      },
      {
        "id": "architectury",
        "version": ">=${architecturyVersion}"
      }
    ]
  }
}