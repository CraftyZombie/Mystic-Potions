{
  "schema_version": 1,
  "mixin": [
    "mysticpotions.mixins.json",
    "mysticpotions-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "mysticpotions",
    "version": "${version}",
    "metadata": {
      "name": "Mystic Potions Quilt",
      "description": "Adds New Potions To Minecraft!",
      "contributors": {
      },
      "contact": {
        "sources": "https://github.com/creep3rcrafter/Mystic-Potions"
      },
      "icon": "assets/mysticpotions/MysticPotions Logo HD720pC.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.creep3rcrafter.mysticpotions.quilt.MysticPotionsQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=${minecraftVersion}"
      },
      {
        "id": "architectury",
        "version": ">=${architecturyVersion}"
      }
    ]
  }
}