{
  "schema_version": 1,
  "mixin": [
    "mysticpotions.mixins.json",
    "mysticpotions-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "mysticpotions",
    "version": "${version}",
    "name": "Mystic Potions (Quilt)",
    "description": "Adds New Potions To Minecraft!",
    "authors": ["Creep3rCrafter"],
    "contact": {
      "sources": "https://github.com/creep3rcrafter/Mystic-Potions"
    },
    "license": "All Rights Reserved",
    "icon": "assets/mysticpotions/MysticPotions Logo HD720pC.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.mysticpotions.quilt.MysticPotionsQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  }
}